{
  "name": "paz-equestrian",
  "version": "1.0.4",
  "description": "A boilerplate for building a fast static website with the Eleventy static site generator, for deployment to Netlify.",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haengle/paz-equestrian"
  },
  "license": "MIT",
  "homepage": "https://github.com/haengle/paz-equestrian",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "clean-css": "^5.3.2",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.3",
    "markdown-it-anchor": "^8.6.7",
    "uglify-js": "^3.17.4"
  }
}
